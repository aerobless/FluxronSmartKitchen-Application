                android:layout_width="fill_parent"
                android:layout_height="wrap_content"/>
            <ch.fluxron.fluxronapp.ui.components.ParameterEditable
                app:editableParamName="@string/F_CCLASS_3030SUB9_BAX_COIL_PEAK_CURRENT"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"/>
            <!-- TODO: 3030-0D Gradient Lim lowT EINFUEGEN - WEGEN BUG NICHT IN PARAM ATM -->
